{
    "window.restoreWindows": "none",
    "window.title": "${rootNameShort}${separator}${profileName}",
    "window.commandCenter": false,
    "window.zoomLevel": 0,
    "window.menuBarVisibility": "toggle",
    "window.titleBarStyle": "native",
    "window.customTitleBarVisibility": "auto",
    "editor.fontFamily": "'Iosevka Medium', 'Cascadia Code', 'Symbols Nerd Font', 'Noto Color Emoji', 'Vazirmatn UI NL', 'Material Icons'",
    "editor.fontWeight": "400",
    "editor.fontSize": 18,
    "editor.formatOnSave": false,
    "editor.formatOnPaste": true,
    "editor.formatOnSaveMode": "modifications",
    "editor.tabSize": 4,
    "editor.mouseWheelZoom": false,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.cursorBlinking": "phase",
    "editor.rulers": [
        80,
        120
    ],
    "editor.wordWrapColumn": 80,
    "editor.renderWhitespace": "all",
    "editor.renderControlCharacters": false,
    "editor.wordBasedSuggestions": "off",
    "editor.suggestSelection": "first",
    "editor.accessibilitySupport": "off",
    "editor.unicodeHighlight.nonBasicASCII": true,
    "editor.unicodeHighlight.includeComments": true,
    "editor.gotoLocation.multipleDeclarations": "gotoAndPeek",
    "editor.gotoLocation.multipleDefinitions": "gotoAndPeek",
    "editor.gotoLocation.multipleTypeDefinitions": "gotoAndPeek",
    "editor.semanticHighlighting.enabled": true,
    "editor.minimap.enabled": false,
    "editor.minimap.scale": 1,
    "editor.minimap.renderCharacters": false,
    "editor.codeLens": true,
    "editor.stickyScroll.scrollWithEditor": false,
    "editor.autoClosingBrackets": "never",
    "editor.copyWithSyntaxHighlighting": false,
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "name": "Comment",
                "scope": [
                    "comment",
                    "comment.block",
                    "comment.block.documentation",
                    "comment.line",
                    "comment.line.double-slash",
                    "punctuation.definition.comment"
                ],
                "settings": {
                    "fontStyle": ""
                }
            }
        ]
    },
    "terminal.external.linuxExec": "kitty",
    "terminal.external.osxExec": "/Applications/kitty.app",
    "terminal.integrated.fontFamily": "'Iosevka Term', 'Symbols Nerd Font', 'Noto Color Emoji', 'Vazir WOL'",
    "terminal.integrated.inheritEnv": true,
    "terminal.integrated.macOptionClickForcesSelection": true,
    "terminal.integrated.macOptionIsMeta": true,
    "terminal.integrated.enableMultiLinePasteWarning": "never",
    "terminal.integrated.shellIntegration.decorationsEnabled": "never",
    "terminal.integrated.minimumContrastRatio": 1,
    "terminal.integrated.defaultProfile.linux": "zsh",
    "terminal.integrated.showExitAlert": false,
    "terminal.integrated.shellIntegration.enabled": false,
    "terminal.integrated.useWslProfiles": false,
    "terminal.integrated.fontSize": 8,
    "terminal.integrated.enableImages": true,
    "workbench.tree.enableStickyScroll": false,
    "workbench.editor.enablePreview": true,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.editor.labelFormat": "short",
    "workbench.editor.decorations.badges": false,
    "workbench.editor.decorations.colors": false,
    "workbench.tips.enabled": false,
    "workbench.layoutControl.enabled": false,
    "workbench.activityBar.orientation": "vertical",
    "workbench.colorCustomizations": {
        "terminal.background": "#11111b",
        "[Catppuccin Mocha]": {
            "terminal.foreground": "#f2e5bc",
            "terminal.ansiBlack": "#d4ba0e",
            "terminal.ansiBrightBlack": "#928374",
            "terminal.ansiRed": "#cc241d",
            "terminal.ansiBrightRed": "#f42c3e",
            "terminal.ansiGreen": "#98971a",
            "terminal.ansiBrightGreen": "#b8bb26",
            "terminal.ansiYellow": "#d79921",
            "terminal.ansiBrightYellow": "#fabd2f",
            "terminal.ansiBlue": "#458588",
            "terminal.ansiBrightBlue": "#99c6ca",
            "terminal.ansiMagenta": "#b16286",
            "terminal.ansiBrightMagenta": "#d3869b",
            "terminal.ansiCyan": "#689d6a",
            "terminal.ansiBrightCyan": "#7ec16e",
            "terminal.ansiWhite": "#a89984",
            "terminal.ansiBrightWhite": "#ebdbb2",
            "sideBar.background": "#060606",
            "sideBarSectionHeader.background": "#060606",
            "focusBorder": "#45475a"
        },
        "[Popping and Locking]": {
            "widget.border": "#fabd2f"
        }
    },
    "workbench.iconTheme": "material-icon-theme",
    "workbench.startupEditor": "none",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "workbench.editor.pinnedTabsOnSeparateRow": false,
    "breadcrumbs.filePath": "off",
    "breadcrumbs.enabled": false,
    "extensions.ignoreRecommendations": true,
    "files.associations": {
        "*.mplstyle": "python"
    },
    "files.trimTrailingWhitespace": true,
    "files.trimFinalNewlines": true,
    "files.insertFinalNewline": true,
    "catppuccin.accentColor": "yellow",
    "catppuccin.colorOverrides": {
        "mocha": {
            "base": "#181828",
            "mantle": "#11111E",
            "crust": "#020202",
            "yellow": "#fabd2f"
        }
    },
    "catppuccin.customUIColors": {
        "mocha": {
            "statusBar.foreground": "accent",
            "sideBarTitle.foreground": "accent",
            "statusBarItem.remoteBackground": "accent",
            "focusBorder": "accent"
        }
    },
    "telemetry.telemetryLevel": "off",
    "update.mode": "start",
    "update.showReleaseNotes": true,
    "explorer.decorations.colors": false,
    "explorer.sortOrder": "type",
    "explorer.incrementalNaming": "smart",
    "explorer.decorations.badges": false,
    "scm.diffDecorationsGutterWidth": 2,
    "scm.diffDecorations": "gutter",
    "git.mergeEditor": true,
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.openDiffOnClick": false,
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "problems.decorations.enabled": false,
    "search.quickOpen.includeHistory": false,
    "[python]": {
        "editor.tabSize": 4,
        "editor.rulers": [
            88,
            120
        ],
        "editor.formatOnSave": true,
        "editor.formatOnSaveMode": "file",
        "editor.codeActionsOnSave": {
            "source.organizeImports": "explicit"
        },
        "editor.defaultFormatter": "charliermarsh.ruff",
        "diffEditor.ignoreTrimWhitespace": false
    },
    "[markdown]": {
        "editor.wordWrap": "bounded",
        "editor.wordWrapColumn": 80,
        "files.trimTrailingWhitespace": false
    },
    "python.languageServer": "Pylance",
    "python.analysis.typeCheckingMode": "standard",
    "python.terminal.activateEnvironment": true,
    "python.testing.pytestEnabled": false,
    "python.testing.unittestEnabled": false,
    "python.testing.autoTestDiscoverOnSaveEnabled": false,
    "remote.SSH.enableX11Forwarding": false,
    "jupyter.askForKernelRestart": false,
    "jupyter.interactiveWindow.textEditor.executeSelection": true,
    "notebook.output.textLineLimit": 200,
    "notebook.lineNumbers": "on",
    "latex-workshop.view.pdf.viewer": "external",
    "latex-workshop.view.pdf.external.viewer.command": "sioyek",
    "latex-workshop.view.pdf.external.synctex.command": "sioyek",
    "latex-workshop.view.pdf.external.synctex.args": [
        "--inverse-search",
        "code -r -g \"%1:%2\"",
        "--reuse-window",
        "--forward-search-file",
        "%TEX%",
        "--forward-search-line",
        "%LINE%",
        "%PDF%"
    ],
    "latex-workshop.message.error.show": false,
    "latex-workshop.message.warning.show": false,
    "latex-workshop.formatting.latexindent.path": "latexindent",
    "latex-workshop.formatting.latexindent.args": [
        "-c",
        "%TMPDIR%/",
        "%TMPFILE%",
        "-y=defaultIndent: '%INDENT%'"
    ],
    "latex-workshop.latex.autoBuild.run": "never",
    "latex-workshop.latex.recipe.default": "first",
    "latex-workshop.latex.outDir": "build",
    "latex-workshop.latex.tools": [
        {
            "name": "make",
            "command": "make"
        }
    ],
    "latex-workshop.latex.recipes": [
        {
            "name": "Makefile",
            "tools": [
                "make"
            ]
        }
    ],
    "git-graph.showStatusBarItem": false,
    "rewrap.autoWrap.enabled": false,
    "markdown.marp.html": "all",
    "markdown.marp.browser": "chrome",
    "markdown.marp.themes": [
        "/home/milad/Documents/presentations/esp_compute/themes/compiled"
    ],
    "accessibility.signals.terminalCommandFailed": {
        "sound": "off"
    },
    "git-blame.config.showBlameInline": false,
    "diffEditor.ignoreTrimWhitespace": true,
    "ruff.nativeServer": true,
    "interactiveWindow.showExecutionHint": false,
    "interactiveWindow.executeWithShiftEnter": true,
    "spellright.notificationClass": "information",
    "spellright.configurationScope": "user",
    "spellright.language": [
        "en-US"
    ],
    "gitlens.codeLens.enabled": false,
    "vscode_custom_css.imports": [
        "file:///home/milad/.dotfiles/linux/.config/Code/User/custom-css.css"
    ],
    "diffEditor.hideUnchangedRegions.enabled": true,
    "cursor.cpp.disabledLanguages": [
        "plaintext",
        "markdown",
        "scminput"
    ],
    "go.toolsManagement.autoUpdate": true,
    "git.openRepositoryInParentFolders": "never",
    "workbench.colorTheme": "Popping and Locking",
    "editor.hover.enabled": false,
}
